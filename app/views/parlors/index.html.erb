<div id="map"></div>

<script type="text/javascript">
    var map;
    function initMap() {
        map = new google.maps.Map(document.getElementById('map'), {
            center: {lat: 35.659487, lng: 139.699901},
            zoom: 17,
            gestureHandling: 'cooperative'
        });
    }
</script>
<%= render 'shared/google_maps_url' %>
