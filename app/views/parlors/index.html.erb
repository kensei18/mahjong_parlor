<div id="map"></div>

<script type="text/javascript">
    var map;
    function initMap() {
        map = new google.maps.Map(document.getElementById('map'), {
            center: {lat: 35.659487, lng: 139.699901},
            zoom: 17,
            gestureHandling: 'cooperative'
        });
    }
</script>
<script
  src="https://maps.googleapis.com/maps/api/js?key=<%= Rails.application.credentials.gmap[:api_key] %>&callback=initMap"
  async defer></script>
